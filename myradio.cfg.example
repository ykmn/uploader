RDSDEVICE=8700i
RDSIP=127.0.0.1
RDSPORT=5001
# RDS Connection port type: TCP or UDP
RDSPORTTYPE=TCP
RDSSITE=www.retrofm.ru
RDSCOMMERCIAL=+7(495)6204664
RDSNONMUSIC=Retro FM

[JSON]
JSONSERVER=http://127.0.0.1/post.php

[FTP1]
