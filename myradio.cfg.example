[Actions]
# Set TRUE to enable action or FALSE to disable
JSON=TRUE
FTP1=TRUE
FTP2=FALSE
RDS=TRUE
PROSTREAM1=TRUE
PROSTREAM2=TRUE

[XML]
# Use XML from Digispot II Value.Server with XML.Writer module with uploader2 and DJin XML Extended 3.0 with uploader3
XMLF=\\server\share\RR-MSK2.xml
#XMLF=C:\Temp\XML\RR-MSK2.xml

[ID]
RARTISTID=7
RTITLEID=17
DEFAULTAT=en
# if ru then RDS receive AT from UserAttribs above


[RDS]
