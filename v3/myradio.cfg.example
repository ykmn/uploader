[Actions]
# Set TRUE to enable action or FALSE to disable
JSON=TRUE
FTP1=TRUE
FTP2=FALSE
RDS=TRUE
PROSTREAM1=TRUE
PROSTREAM2=TRUE

[XML]
# Using XML from Digispot II Value.Server with XML.Writer module or XML 
XMLF=\\server\share\RR-MSK2.xml
#XMLF=C:\Temp\XML\RR-MSK2.xml

[ID]
# EP: 8/19/FALSE
# RR: 7/17/TRUE
ALTARTISTID=7
ALTTITLEID=17
ALTAT=TRUE
# TRUE, когда в основных атрибутах находится транслит, а в пользовательских - русские AT.
# FALSE, если в основных атрибутах находятся русские AT.


[RDS]
# RDS-coder type: 8700i or SmartGen
RDSDEVICE=8700i
RDSIP=127.0.0.1
RDSPORT=5001
# RDS Connection port type: TCP or UDP
RDSPORTTYPE=TCP
# адрес сайта, выводится в RT после названия песни
RDSSITE=www.retrofm.ru
# телефон коммерческой службы, выводится в RT во время рекламы
RDSCOMMERCIAL=+7(495)6204664
# текст который выводится в RT если не песня и не реклама
RDSNONMUSIC=Retro FM

[JSON]
JSONSERVER=http://127.0.0.1/post.php

[FTP1]
FTPSERVER1=127.0.0.1:30021
FTPUSER1=user1
FTPPASS1=pass1
FTPPATH1=/pub/uploads/Radio1/

[FTP2]
FTPSERVER2=127.0.0.1:21
FTPUSER2=user2
FTPPASS2=pass2
FTPPATH2=/

[PROSTREAM]
# Using Omnia ProStream "Character Parser Sample" filter
ZIPSERVER1=127.0.0.1
ZIPPORT1=6001
ZIPSERVER2=127.0.0.1
ZIPPORT2=6001
